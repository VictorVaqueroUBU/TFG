{% extends '../../base.html.twig' %}

{% block title %}Curso{% endblock %}

{% block body %}
    <h1>Curso</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ curso.id }}</td>
            </tr>
            <tr>
                <th>Codigo_curso</th>
                <td>{{ curso.codigoCurso }}</td>
            </tr>
            <tr>
                <th>Nombre_curso</th>
                <td>{{ curso.nombreCurso }}</td>
            </tr>
            <tr>
                <th>Horas</th>
                <td>{{ curso.horas }}</td>
            </tr>
            <tr>
                <th>Objetivos</th>
                <td>{{ curso.objetivos }}</td>
            </tr>
            <tr>
                <th>Contenidos</th>
                <td>{{ curso.contenidos }}</td>
            </tr>
            <tr>
                <th>Destinatarios</th>
                <td>{{ curso.destinatarios }}</td>
            </tr>
            <tr>
                <th>Requisitos</th>
                <td>{{ curso.requisitos }}</td>
            </tr>
            <tr>
                <th>Justificacion</th>
                <td>{{ curso.justificacion }}</td>
            </tr>
            <tr>
                <th>Coordinador</th>
                <td>{{ curso.coordinador }}</td>
            </tr>
            <tr>
                <th>Participantes_edicion</th>
                <td>{{ curso.participantesEdicion }}</td>
            </tr>
            <tr>
                <th>Ediciones_estimadas</th>
                <td>{{ curso.edicionesEstimadas }}</td>
            </tr>
            <tr>
                <th>Plazo_solicitud</th>
                <td>{{ curso.plazoSolicitud }}</td>
            </tr>
            <tr>
                <th>Observaciones</th>
                <td>{{ curso.observaciones }}</td>
            </tr>
            <tr>
                <th>Visible_web</th>
                <td>{{ curso.visibleWeb ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Id_programa</th>
                <td>{{ curso.idPrograma }}</td>
            </tr>
            <tr>
                <th>Horas_virtuales</th>
                <td>{{ curso.horasVirtuales }}</td>
            </tr>
            <tr>
                <th>Calificable</th>
                <td>{{ curso.calificable ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_curso_index') }}">back to list</a>

    <a href="{{ path('app_curso_edit', {'id': curso.id}) }}">edit</a>

    {{ include('curso/_delete_form.html.twig') }}
{% endblock %}
